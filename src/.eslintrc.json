{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    
    "extends": [
      "eslint:recommended",
      "plugin:github/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"]
    }
    
  }